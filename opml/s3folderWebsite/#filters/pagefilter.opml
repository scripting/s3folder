	<body>
		<outline text="on pageFilter (pta)">
			<outline text="if typeOf (pta^.template) == outlineType or typeOf (pta^.template) == wpTextType">
				<outline text="table.assign (@pta^.template, string (pta^.template))"/>
				</outline>
			<outline text="if typeOf (pta^.template) == stringType and not pta^.indirectTemplate">
				<outline text="pta^.template = string.replace (pta^.template, &quot;&lt;title&gt;&quot;, &quot;[[title]]&quot;)"/>
				</outline>
			<outline text="html.addPageToGlossary (pta)"/>
			<outline text="return (true)"/>
			</outline>
		</body>
